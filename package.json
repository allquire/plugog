{
  "name": "plugog",
  "version": "0.1.0",
  "description": "Plugin Logging Utility.",
  "main": "dist/index.ts",
  "scripts": {
    "dev": "ts-node index.ts",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore . --fix",
    "build": "tsc -b",
    "test": "jest",
    "commit": "git cz"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allquire/plugog.git"
  },
  "keywords": [
    "plugin",
    "log",
    "require",
    "extension",
    "chalk",
    "console",
    "consola"
  ],
  "lint-staged": {
    "*.{js,ts}": "eslint --ext .js,.ts --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "Il Harper <afanyiyu@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allquire/plugog/issues"
  },
  "homepage": "https://github.com/allquire/plugog#readme",
  "devDependencies": {
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "git-cz": "^3.2.1",
    "husky": "^3.0.9",
    "jest": "^25.1.0",
    "lint-staged": "^9.3.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
